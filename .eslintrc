{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "classes": true
  },
  "settings": {
    "import/resolver": {
      "react-native": {},
      "alias": {
        "map": [["~", "./modules"]]
      }
    }
  },
  "plugins": ["react", "import"],
  "rules": {
    "import/no-unresolved": 2,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
